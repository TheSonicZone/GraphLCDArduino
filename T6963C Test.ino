//----------------------------------------------------------
// Name: PutGraphic
// Function: Write image to graphics memory
//-----------------------------------------------------------
void PutGraphic(char *bmpdata){
  unsigned int n, p, v;
  unsigned char selection = FS;


  if(selection == 1){
    v = 22;   // FS pin = 1, this means 24 bytes = 1 line across (where each byte's two MSBs are disregarded)
  } else {
    v = 16;     // FS pin = 0, this means 16 bytes = 1 line across
  }

  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x02);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x24);    // Command Code

  // Set AUTO-WRITE mode
  digitalWrite(C_D, HIGH);
  BusWrite(0xB0);    // Command Code  

  bmpaddress = 0;
  for (p = 0; p < 128; p++){
    // One row of pixels across = 128 bits... therefore 128 / 8 = 16
    for (n = 0; n < 16; n++){
      CheckLCDReady_2();
      digitalWrite(C_D, LOW);
      BusWrite((char *)pgm_read_word(&(bmpdata[bmpaddress])));               // Data to be written to memory... 
      bmpaddress++;
    }
    
  }
  // Clear AUTO-WRITE command
  digitalWrite(C_D, HIGH);
  BusWrite(0xB2);    // Command Code  
}


//--------------------------------------------------------
// Name: ReadLCDStatus
// Function: Read the T6963's status bits
//--------------------------------------------------------
unsigned char ReadLCDStatus(void){

  digitalWrite(C_D, HIGH);    // Command/Data pin = 1
  return BusRead();

}

//--------------------------------------------------------
// Name: CheckLCDReady
// Function: Check STA0 and STA1 bits in T6963, 
// and when both = 1, return
//---------------------------------------------------------
void CheckLCDReady(void){
    unsigned char tmp;
    for(;;){
    tmp = ReadLCDStatus();
 //   sprintf(buffer, "T6963 STA: %02X", tmp);
 //   Serial.println(buffer);

    if ((tmp & 0x03) == 0x03){
      return;
    }
  }
}

//--------------------------------------------------------
// Name: CheckLCDReady_2
// Function: Check STA3 bit only, 
// and when = 1, return
//---------------------------------------------------------
void CheckLCDReady_2(void){
    unsigned char tmp;
    for(;;){
    tmp = ReadLCDStatus();
 //   sprintf(buffer, "T6963 STA TYPE 2: %02X", tmp);
 //   Serial.println(buffer);
    if ((tmp & 0x08) == 0x08){
      return;
    }
  }
}

//---------------------------------------------------------
// Name: InitT6963
// Function: Initialise the T6963C into operational mode
// Parameters: void
//---------------------------------------------------------
void InitT6963(void){

  // Set text home address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x40);    // Command Code

  // Set graphics home address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x02);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x42);    // Command Code

  // Set text area
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x16);     // 0x0016 means the lines are 21 chars wide, the last char must be a space to avoid extraneous pixels
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x41);    // Command Code

  // Set graphics area
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(16);     // low byte of address 0x10 = 16 = 16 columns of 1 byte = 128 pixels // 22 for 6x8
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x43);    // Command Code

  // Set Display Mode
  digitalWrite(C_D, HIGH);
  BusWrite(0x80);    // Command Code   (OR'ed mode with internal character generator)

  // Set Offset register
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x02);     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x22);    // Command Code

  ClearTextMemory();
  ClearGraphicMemory();

  // Turn LCD on
  digitalWrite(C_D, HIGH);
  BusWrite(0x9C);    // Command Code  

}

//----------------------------------------------------------
// Name: PutText
// Function: Write a string to the LCD controller memory
// Note: Uses auto-increment function of the hardware
//----------------------------------------------------------
void PutText(char *string, unsigned int x, unsigned int y){

  unsigned int n = 0;
  unsigned int offset = y * 22 + x;

  // Set address pointer in LCD controller
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(*(((char *)&offset)+0));     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(*(((char *)&offset)+1));     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x24);    // Command Code

   // Set AUTO-WRITE mode
  digitalWrite(C_D, HIGH);
  BusWrite(0xB0);    // Command Code  

  while(string[n] != 0){        // While until null-terminated char is encountered
    CheckLCDReady_2();
    digitalWrite(C_D, LOW);
    BusWrite(string[n] - 0x20);
    n++;    
  }
  // Clear AUTO-WRITE mode
  digitalWrite(C_D, HIGH);
  BusWrite(0xB2);    // Command Code  

}
//----------------------------------------------------------
// Name: ClearTextMemory
// Function: Clear the areas in memory used for text display
//-----------------------------------------------------------
void ClearTextMemory(void){
  unsigned int n, tmp;

  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x24);    // Command Code

  // Set AUTO-WRITE mode
  digitalWrite(C_D, HIGH);
  BusWrite(0xB0);    // Command Code  

  for (n = 0; n < 384; n++){
    CheckLCDReady_2();
    digitalWrite(C_D, LOW);
    BusWrite(0x00);
  }

  // Clear AUTO-WRITE command
  digitalWrite(C_D, HIGH);
  BusWrite(0xB2);    // Command Code  
}

//----------------------------------------------------------
// Name: ClearGraphicMemory
// Function: Clear the areas in memory used for graphics
//-----------------------------------------------------------
void ClearGraphicMemory(void){
  unsigned int n, p, v;
  unsigned char tmp = 0x30;
  unsigned char selection = FS;

  if(selection == 1){
    v = 22;   // FS pin = 1, this means 24 bytes = 1 line across (where each byte's two MSBs are disregarded)
  } else {
    v = 16;     // FS pin = 0, this means 16 bytes = 1 line across
  }

  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x00);     // low byte of address
  CheckLCDReady();
  digitalWrite(C_D, LOW);
  BusWrite(0x02);     // high byte of address
  CheckLCDReady();
  digitalWrite(C_D, HIGH);
  BusWrite(0x24);    // Command Code

  // Set AUTO-WRITE mode
  digitalWrite(C_D, HIGH);
  BusWrite(0xB0);    // Command Code  

  for (p = 0; p < 128; p++){
    // One row of pixels across = 128... therefore 128 / 8 = 16
    for (n = 0; n < 16; n++){
      CheckLCDReady_2();
      digitalWrite(C_D, LOW);
      BusWrite(0x00);               // Data to be written to memory... 
    }
    tmp = 0;
  }
  // Clear AUTO-WRITE command
  digitalWrite(C_D, HIGH);
  BusWrite(0xB2);    // Command Code  
}


//---------------------------------------------------------
// Name: BusRead
// Function: Read a byte from the bit-banged 8-bit MPU bus
// Returns: Byte written
//---------------------------------------------------------
unsigned char BusRead(void){

  unsigned char data = 0;

  // Change D0-D7 to inputs
  pinMode(D0, INPUT);
  pinMode(D1, INPUT);
  pinMode(D2, INPUT);
  pinMode(D3, INPUT);
  pinMode(D4, INPUT);
  pinMode(D5, INPUT);
  pinMode(D6, INPUT);
  pinMode(D7, INPUT);

  // Start read cycle
  digitalWrite(CE, LOW);
  digitalWrite(RD, LOW);

  if(digitalRead(D0)){
    data |= 0x01;
  }
  
  if(digitalRead(D1)){
    data |= 0x02;
  }

  if(digitalRead(D2)){
    data |= 0x04;
  }

  if(digitalRead(D3)){
    data |= 0x08;
  }

  if(digitalRead(D4)){
    data |= 0x10;
  }

  if(digitalRead(D5)){
    data |= 0x20;
  }

  if(digitalRead(D6)){
    data |= 0x40;
  }

  if(digitalRead(D7)){
    data |= 0x80;
  }

  // End read cycle
  digitalWrite(RD, HIGH);
  digitalWrite(CE, HIGH);

  // Restore D0-D7 output mode
  pinMode(D0, OUTPUT);
  pinMode(D1, OUTPUT);
  pinMode(D2, OUTPUT);
  pinMode(D3, OUTPUT);
  pinMode(D4, OUTPUT);
  pinMode(D5, OUTPUT);
  pinMode(D6, OUTPUT);
  pinMode(D7, OUTPUT);

  return data;
}

//---------------------------------------------------------
// Name: BusWrite
// Function: Write a byte to the bit-banged 8-bit MPU bus
// Parameters: Byte to be written
//---------------------------------------------------------
void BusWrite(unsigned char data){

  // Take all pins low at first... 
  digitalWrite(D0, LOW);
  digitalWrite(D1, LOW);
  digitalWrite(D2, LOW);
  digitalWrite(D3, LOW);
  digitalWrite(D4, LOW);
  digitalWrite(D5, LOW);
  digitalWrite(D6, LOW);
  digitalWrite(D7, LOW);

  // Place byte on the bus
  if ((data & 0x80) == 0x80){
    digitalWrite(D7, HIGH);
  }
    if ((data & 0x40) == 0x40){
    digitalWrite(D6, HIGH);
  }
    if ((data & 0x20) == 0x20){
    digitalWrite(D5, HIGH);
  }
    if ((data & 0x10) == 0x10){
    digitalWrite(D4, HIGH);
  }
    if ((data & 0x08) == 0x08){
    digitalWrite(D3, HIGH);
  }
    if ((data & 0x04) == 0x04){
    digitalWrite(D2, HIGH);
  }
    if ((data & 0x02) == 0x02){
    digitalWrite(D1, HIGH);
  }
    if ((data & 0x01) == 0x01){
    digitalWrite(D0, HIGH);
  }

  // Generate Write
  digitalWrite(CE, LOW);
  digitalWrite(WR, LOW);
  digitalWrite(WR, HIGH);
  digitalWrite(CE, HIGH);

}


